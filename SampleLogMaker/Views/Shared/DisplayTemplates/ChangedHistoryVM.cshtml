@using SampleLogMaker.ViewModels
@model ChangedHistoryVM

<div class="changed">
    @Model.UserName changed 

   

    <span class="historySpan changedHistoySpan tooltipper" title=" <div>
        @foreach (var item in Model.Details)
        {
            <div>
                @item.PropertyName : @item.OldValue -> @item.NewValue
            </div>
        }
    </div>">
        @Model.TableName # @Model.RecordId
    </span>
    
     from
    on @(Model.Date.ToString(Constants.DateFormat)).
</div>
